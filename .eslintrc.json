{
    "extends": [
        "standard",
        "plugin:jest/recommended"
    ],

    "plugins": [
        "import",
        "jest"
    ],

    "env": {
        "node": true,
        "jest/globals": true
    },

    "rules": {
        "quotes": "off",
        "semi": "off",
        "indent": "off",
        "no-trailing-spaces": "off"
    }
}